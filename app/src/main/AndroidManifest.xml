<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.microsoft_tech_club_isimm">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/mtc_white_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mtc_white_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Microsoft_Tech_Club_ISIMM">
        <activity android:name=".DetailsTraining" />
        <activity android:name=".DetailsEvents" />
        <activity android:name=".Member_of_the_month" />
        <activity android:name=".Formations" />
        <activity android:name=".Events" />
        <activity android:name=".Team" />
        <activity android:name=".AboutUs" />
        <activity android:name=".Home_Page" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>